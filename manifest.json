{
  "manifest_version": 3,
  "name": "My First Extension",
  "version": "1.0",
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "description": "CG T2 notes  extension.",
  "permissions": ["scripting", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],

  "action": {
    "default_popup": "index.html",
    "default_icon": "icon32.png"
  }
}
